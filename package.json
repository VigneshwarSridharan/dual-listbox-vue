{
  "name": "dual-listbox-vue",
  "version": "1.0.2",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name dual-listbox ./src/components/DualListBox.vue",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/dual-listbox.umd.js",
  "unpkg": "dist/dual-listbox.umd.min.js",
  "browser": {
    "./sfc": "src/components/DualListBox.vue"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-eslint": "3.6.0",
    "@vue/cli-service": "3.6.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "dual list box",
    "listbox",
    "dual listbox",
    "muliselect",
    "move select",
    "double list box",
    "select box"
  ],
  "description": "",
  "author": "Vigneshwar Sridharan",
  "license": "MIT",
  "homepage": "https://github.com/VigneshwarSridharan/dual-listbox-vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/VigneshwarSridharan/dual-listbox-vue"
  }
}
